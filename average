/*average*/
#[macro_use]
extern crate text_io; 
 
 
fn main() {
    println!("The 1st number is");
    let num1:f32 = read!();
    println!("The 2nd number is");
    let num2:f32 = read!();
    println!("The 3rd number is");
    let num3:f32 = read!();
    let sum:f32 = num1 + num2 + num3;
    let average:f32 = sum/3.0;
    println!("The average is {}",average);
}
